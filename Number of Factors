import java.util.Scanner;
class numoffactors {
public static void main(String[] args)
{
try
{
Scanner input=new Scanner(System.in);
System.out.print("Enter n: ");
int n=input.nextInt();
int factors=0;
for(int i=1;i<=n;i++)
{
 if(n%i==0)
 factors=factors+1;
}
System.out.print("Number of factors = "+factors);
}
catch(Exception e)
{
System.out.println(" enter valid");
}
}}
