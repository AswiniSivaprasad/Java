import java.util.*;
import java.util.Scanner;
class pattern23
{
public static void main(String[] args)
{
Scanner input=new Scanner(System.in);
System.out.print("enter the no of rows: ");
int n=input.nextInt();
for(int i=1;i<=n;i++)
{
 for(int j=1;j<=i;j++)
 {
 System.out.print(i);
 }
 System.out.print("\n");
}
}
}
