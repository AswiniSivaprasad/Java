import java.util.*;
public class nfactors
{
public static void main(String[] args)
{
int num,n;
Scanner sc = new Scanner(System.in);
System.out.print("Enter a number : ");
num = sc.nextInt();
System.out.print("Enter N:");
n= sc.nextInt();
int i, count = 0;
for(i = 1; i <= num; i++)
{
if(num % i == 0)
{
count = count + 1;
if(count==n)
{
 System.out.print(n+"th Factor is "+i);
}
}
}
System.out.print("\nNumber of factors of " + num + " : " + count);
}
}
