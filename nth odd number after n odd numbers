import java.util.Scanner;
class nthodd {
public static void main(String[] args)
{
try
{
Scanner input=new Scanner(System.in);
System.out.print("Enter N: ");
int n=input.nextInt();
int arr[]=new int[100];
int j=1;
for(int i=1;i<100;i++)
{
 if(i%2!=0) {
 arr[j] = i;
 j++;
 }
}
System.out.print("The odd number is: "+arr[n*2]);
}
catch(Exception e)
{
System.out.println(" enter valid");
}
}}
